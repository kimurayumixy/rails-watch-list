<div class="banner" style="background-image: linear-gradient(rgba(0,0,0,0.4),rgba(0,0,0,0.4)), url(https://www.justwatch.com/images/backdrop/207404040/s640/moana);">
  <div class="container">
    <h1><%= @list.name %></h1>
  </div>
</div>
<div class="container mt-3">
  <h2>movies & reviews</h2>
  <% @list.bookmarks.each do |bookmark| %>
    <div class="card-product mt-3">
      <img src="data:image/jpeg;base64," />
      <div class="card-product-infos">
        <h2><%= bookmark.movie.title %></h2>
        <p><%= bookmark.comment %></p>
        <%= link_to bookmark_path(bookmark), data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}, class: 'card-remove-link' do %>
          <i class="fa-solid fa-trash"></i>
        <% end %>
      </div>
    </div>
  <% end %>
  <p><%= link_to "Back to list", lists_path %></p>
  <p><%= link_to "Add new bookmark", new_list_bookmark_path(@list) %></p>
</div>
